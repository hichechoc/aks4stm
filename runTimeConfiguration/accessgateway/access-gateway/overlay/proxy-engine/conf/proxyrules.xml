<nete:proxyrules xmlns:nete="https://sps.tanzulab.it/">

    <!-- Host principale -->
    <nete:cond type="host">
        <nete:case value="sps.tanzulab.it">

            <!-- Regole per i path -->
            <nete:cond type="uri" criteria="equals">
                <nete:case value="/protected/case1">
                    <nete:forward>https://www.broadcom.com/</nete:forward>
                </nete:case>

                <nete:default>
                    <nete:forward>https://www.inail.it/</nete:forward>
                </nete:default>
            </nete:cond>

        </nete:case>

        <!-- Default host fallback -->
        <nete:default>
            <nete:forward>https://www.google.com/</nete:forward>
        </nete:default>
    </nete:cond>

</nete:proxyrules>
