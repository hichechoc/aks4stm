<?xml version="1.0"?>
<?cocoon-process type="xslt"?>

<!DOCTYPE nete:proxyrules SYSTEM "file:////opt/CA/secure-proxy/proxy-engine/conf/dtd/proxyrules.dtd">
<!-- Proxy Rules -->
<!-- replace www.example.com with your namespace -->
<nete:proxyrules xmlns:nete="https://sps.akslab.com/">
	<nete:cond criteria="beginswith" type="uri">
		<nete:case value="/protected">
				<nete:forward>https://www.broadcom.com/</nete:forward>
		</nete:case>
		<nete:default>
			<nete:forward>https://www.google.com</nete:forward>
		</nete:default>		
	</nete:cond>
</nete:proxyrules>
